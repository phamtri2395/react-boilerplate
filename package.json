{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./index.js",
    "clean": "rimraf ./public/dists",
    "build:prod": "npm run clean && cross-env NODE_ENV=production webpack --progress --colors --config ./utils/webpack.config.prod.js",
    "build:dev": "npm run clean && cross-env NODE_ENV=production webpack --progress --colors --config ./utils/webpack.config.dev.js",
    "build": "npm run build:prod",
    "lint": "./node_modules/.bin/eslint ./client/"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "cross-env": "^3.1.3",
    "debug": "~2.2.0",
    "ejs": "~2.4.1",
    "express": "~4.13.4",
    "morgan": "~1.7.0",
    "node-sass-middleware": "0.8.0",
    "serve-favicon": "~2.3.0"
  },
  "author": "phamtri2395",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.8.1",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "jquery": "^3.1.1",
    "lodash": "^4.16.4",
    "node-sass": "^3.10.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-lite": "^0.15.26",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  }
}
